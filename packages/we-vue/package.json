{
  "name": "we-vue",
  "version": "3.0.0-alpha.9",
  "description": "Vue components based on Vue2.x and weui1.x",
  "keywords": [
    "vue",
    "weui",
    "we-vue",
    "component"
  ],
  "author": "tianyong90 <412039588@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tianyong90/we-vue.git"
  },
  "homepage": "https://wevue.org",
  "bugs": {
    "url": "https://github.com/tianyong90/we-vue/issues"
  },
  "license": "MIT",
  "main": "dist/we-vue.js",
  "module": "dist/we-vue.js",
  "types": "types/index.d.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack --config build/webpack.dev.conf.js --progress --hide-modules --colors --watch",
    "lint": "eslint --ext .js,.ts,.tsx,.vue src test/unit/",
    "lint:style": "stylelint src/scss",
    "build": "concurrently \"yarn build:dist --no-progress\" \"yarn build:lib\" -n \"dist,lib\" --kill-others-on-fail -r",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config build/config.js --progress --hide-modules",
    "build:lib": "node build/build-lib.js",
    "test": "jest --config=jest.config.js --colors",
    "test:types": "tsc -p ./types/test/tsconfig.json",
    "test:watch": "jest --config=jest.config.js --colors --watchAll",
    "test:coverage": "jest --config=jest.config.js --colors --coverage",
    "coverage": "yarn codecov",
    "postinstall": "rimraf node_modules/.cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "vue-lazyload": "^1.2.0"
  },
  "peerDependencies": {
    "vue": ">= 2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/dom-inputevent": "^1.0.3",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.3",
    "@types/lodash": "^4.14.119",
    "@types/lolex": "^3.1.0",
    "@types/node": "^12.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.0.0",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-define": "^1.3.1",
    "cache-loader": "^3.0.0",
    "clean-webpack-plugin": "^2.0.1",
    "codecov": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^22.5.0",
    "eslint-plugin-node": "^9.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "faker": "^4.1.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^8.0.0",
    "husky": "^2.1.0",
    "jest": "^24.1.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "jest-serializer-vue": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^8.0.0",
    "lodash": "^4.17.11",
    "lolex": "^4.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.14.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.0",
    "sass": "^1.17.0",
    "sass-loader": "^7.0.0",
    "terser-webpack-plugin": "^1.2.2",
    "thread-loader": "^2.1.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.0",
    "url-loader": "^1.0.1",
    "vue": "^2.5.9",
    "vue-jest": "^3.0.3",
    "vue-template-compiler": "^2.6.8",
    "vue-tsx-support": "^2.2.2",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
